<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="GenSci.FamilyBudget.DesktopApp.Views.Controls.SummaryControl">
  <Grid Margin="20">
    <Grid.RowDefinitions>
      <RowDefinition Height="0.1*"></RowDefinition>
      <RowDefinition></RowDefinition>
      <RowDefinition></RowDefinition>
    </Grid.RowDefinitions>
    <StackPanel Orientation="Horizontal">
      <ComboBox Items="{Binding Months}" SelectedItem="{Binding CurrentMonth}" Width="150">
      </ComboBox>
      <ComboBox  Items="{Binding Years}" SelectedItem="{Binding CurrentYear}" Width="100">
      </ComboBox>
    </StackPanel>

  </Grid>
</UserControl>
